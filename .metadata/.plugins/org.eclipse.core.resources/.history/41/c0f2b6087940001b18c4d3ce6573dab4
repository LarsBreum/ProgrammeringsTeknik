
public class Hello {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		int start = 101;
		int stop = 125;
		int interval = 2;
		int digit;
		int res[] = new int[10];
		
		for(int i = start; i <= stop; i+=interval) {
			int num = i;
			
			while(num >= 0) {
				digit = num%10;
				res[digit]++;
			}
			
			
		}
		
		for(int i = 0; i <= res.length-1; i++) {
			System.out.println(res[i]);
		}
		
	}

}
