
public class Player {
	private int health;
	private int strength;
	private int defense;
	private String weapon;
	
	Player (){
		health = 0;
		strength = 0;
		defense = 0;
		weapon = "Sword";
	}
	public int getHealth(){
		return(health);
	}
	public int getStrength(){
		return(strength);
	}
	public int getDefense(){
		return(defense);
	}
	public int getWeapon(){
		return(weapon);
	}
	
	
}
