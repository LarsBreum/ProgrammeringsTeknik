import se.lth.cs.pt.window.SimpleWindow;

public class Graphics {
	
	private int width;
	private int height;
	private int blockSize;
	
	Graphics(int w, int h, int bs) {
		width = w;
		height = h;
		blockSize = bs;
	}
	
	private SimpleWindow w = new SimpleWindow(300, 500, "Digging");

	/**
	 * @param args
	 */
	public void square() {
		w.moveTo(10, 10);
		w.lineTo(10, 20);
		w.lineTo(20, 20);
		w.lineTo(20, 10);
		w.lineTo(10, 10);
		

	}

}
