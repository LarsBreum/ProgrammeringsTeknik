public class Mole {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Graphics g = new Graphics(30, 50, 10);
		g.square();
		
		g.block(10,10, Colors.MOLE);
		g.block(20,20, Colors.MOLE);
		g.block(25,10, Colors.MOLE);
		
	}

}
