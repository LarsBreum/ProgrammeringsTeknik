
public class Hello {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		int start = 101;
		int stop = 125;
		int interval = 2;
		
		int res[] = new int[10];
		
		for(int i = start; i <= stop; i+=interval) {
			int num = i;
			
			int digit = num%10;
			res[digit]++;
			
			
		}
		
		
		
	}

}
